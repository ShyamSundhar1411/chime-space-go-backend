{
    "swagger": "2.0",
    "info": {
        "description": "API documentation for ChimeSpace backend",
        "title": "ChimeSpace API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/chimes": {
            "get": {
